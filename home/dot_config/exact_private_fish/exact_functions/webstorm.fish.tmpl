# chezmoi:template:left-delimiter="# {{" right-delimiter=}}
# {{- if and .device.wsl (lookPath "webstorm" | hasPrefix "/mnt/c/") -}}
# Temporary workaround for https://github.com/fish-shell/fish-shell/issues/11354
function webstorm
    if command -q webstorm
        command webstorm $argv
    else
        "# {{ lookPath "webstorm" }}" $argv
    end
end
# {{ else if eq .chezmoi.os "darwin" -}}
function webstorm --wraps=/Applications/WebStorm.app/Contents/MacOS/webstorm --description 'alias webstorm=/Applications/WebStorm.app/Contents/MacOS/webstorm'
    /Applications/WebStorm.app/Contents/MacOS/webstorm $argv
end
# {{ end -}}
