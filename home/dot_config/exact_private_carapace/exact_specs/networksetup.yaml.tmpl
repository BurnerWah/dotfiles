# yaml-language-server: $schema=https://carapace.sh/schemas/command.json
name: networksetup
description: Configuration tool for network settings in System Preferences
parsing: disabled
completion:
  positionalany:
    - '
      {{- if lookPath "inshellisense" -}}
      $carapace.bridge.Inshellisense([networksetup])
      {{- else -}}
      $carapace.bridge.Zsh([networksetup])
      {{- end -}}
      '
