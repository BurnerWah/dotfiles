{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua",
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[snippets]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "files.associations": {
    "**/*_dot_ssh/*_config.d/*.conf": "ssh_config",
    "**/*_dot_ssh/*_config.d/*.conf.tmpl": "ssh_config",
    "**/*_dot_ssh/*config": "ssh_config",
    "**/*_dot_ssh/*config.tmpl": "ssh_config",
    "**/.chezmoitemplates/zsh/**": "shellscript",
    "**/dot_config/*_bat/config": "plaintext",
    "**/dot_config/*_fd/ignore": "ignore",
    "**/dot_config/*_go/*env.tmpl": "dotenv",
    "**/dot_config/*_litecli/config.tmpl": "ini",
    "**/dot_config/*_rainbow/*.cfg": "plaintext",
    "**/dot_config/*_zsh/*_functions/**": "shellscript",
    "*.desktop": "ini",
    "*.elv.tmpl": "elvish",
    "*.env.tmpl": "dotenv",
    "*.fish.tmpl": "fish",
    "*.json.tmpl": "go-template",
    "*.plist": "xml",
    "*.plist.tmpl": "xml",
    "*.ps1.tmpl": "powershell",
    "*.sh.tmpl": "shellscript",
    "*.sublime-syntax": "yaml",
    "*.toml.tmpl": "toml",
    "*.yaml.tmpl": "yaml",
    "*.yml.tmpl": "yaml",
    "*.zsh.tmpl": "shellscript",
    ".chezmoiignore": "ignore",
    ".chezmoiremove": "ignore",
    "ansible.cfg.tmpl": "ini",
    "dot_bashrc": "shellscript",
    "dot_zshrc": "shellscript",
  },
  "gitconfig-lsp.format.gitattributes.tabSize": 16,
  "go-template.patterns": [
    "**/*.tmpl",
    "**/.chezmoiignore",
    "**/.chezmoiremove",
    "**/.chezmoitemplates/**/*",
    "**/modify_*",
  ],
  "nushellLanguageServer.includeDirs": [
    "./home/dot_config/exact_private_nushell/modules",
  ],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.analysis.extraPaths": [
    "/opt/homebrew/opt/xonsh/libexec/lib/python3.13/site-packages",
  ],
  "python.autoComplete.extraPaths": [
    "/opt/homebrew/opt/xonsh/libexec/lib/python3.13/site-packages",
  ],
  "yaml.schemas": {
    "./schemas/chezmoi/config.yaml": ["home/.chezmoi.yaml.tmpl"],
    "./schemas/chezmoi/data.yaml": [
      "**/.chezmoidata.yaml",
      "**/.chezmoidata/**/*",
    ],
    "./schemas/chezmoi/externals.yaml": ["home/.chezmoiexternals/**/*"],
    "./schemas/vendor/lima.json": ["home/dot_config/private_lima/*/lima.yaml"],
    "https://carapace.sh/schemas/command.json": [
      "home/dot_config/carapace/exact_specs/*.yaml",
      "home/dot_config/carapace/exact_overlays/*.yaml",
    ],
    "https://github.com/sharkdp/vivid/raw/refs/heads/master/schemas/filetypes.json": [
      "home/dot_config/vivid/filetypes.yml",
    ],
    "https://github.com/sharkdp/vivid/raw/refs/heads/master/schemas/theme.json": [
      "home/dot_config/vivid/themes/*",
    ],
    "https://json.schemastore.org/lefthook.json": [
      "lefthook.yml",
      "lefthook.yaml",
      "lefthook-local.yml",
      "lefthook-local.yaml",
    ],
  },
}
