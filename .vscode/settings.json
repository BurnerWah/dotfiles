{
  "files.associations": {
    "**/.chezmoitemplates/zsh/**": "shellscript",
    "**/dot_config/private_zsh/functions/**": "shellscript",
    "**/dot_config/private_rainbow/*.cfg": "plaintext",
    "*.fish.tmpl": "fish",
    "*.toml.tmpl": "toml",
    "*.yaml.tmpl": "yaml",
    "*.yml.tmpl": "yaml",
    "*.env.tmpl": "dotenv",
    ".chezmoiignore": "ignore",
    "*.sublime-syntax": "yaml",
    "dot_zshrc": "shellscript",
    "dot_bashrc": "shellscript",
    "*.sh.tmpl": "shellscript",
    "*.zsh.tmpl": "shellscript",
    "**/dot_config/bat/config": "plaintext",
    "*.ps1.tmpl": "powershell",
    "*.plist": "xml",
    "*.plist.tmpl": "xml",
    ".chezmoiremove": "ignore",
    "**/litecli/config.tmpl": "ini",
    "**/private_go/*env.tmpl": "dotenv",
  },
  "go-template.patterns": [
    "**/*.tmpl",
    "**/.{chezmoiignore,chezmoiremove}",
    "**/.chezmoitemplates/**/*",
    "**/modify_*",
    "**/{.mise,mise}.toml",
    "**/.mise.*.toml",
    "**/{.mise,mise}/config.toml",
  ],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "yaml.schemas": {
    "https://carapace.sh/schemas/command.json": [
      "home/dot_config/carapace/exact_specs/*.yaml",
      "home/dot_config/carapace/exact_overlays/*.yaml",
    ],
    "./schemas/chezmoi/data.yaml": [
      "**/.chezmoidata.yaml",
      "home/.chezmoidata/**/*",
    ],
    "./schemas/chezmoi/externals.yaml": ["home/.chezmoiexternals/**/*"],
    "./schemas/chezmoi/config.yaml": ["home/.chezmoi.yaml.tmpl"],
  },
  "python.autoComplete.extraPaths": [
    "/opt/homebrew/opt/xonsh/libexec/lib/python3.12/site-packages",
  ],
  "python.analysis.extraPaths": [
    "/opt/homebrew/opt/xonsh/libexec/lib/python3.12/site-packages",
  ],
  "nushellLanguageServer.includeDirs": [
    "./home/dot_config/private_nushell/modules",
  ],
}
